{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Generate All": {
      "commandName": "Project",
      "targetProject": "../GenerateSlang.cs",
      "commandLineArgs": "--slang-repo \".\\slang\" --bindings-output \".\\ShaderSlang.Net.Bindings\\Generated\" --tests-output \".\\ShaderSlang.Net.Tests\\Generated\" --docs-output \".\\ShaderSlang.Net.Bindings\\Generated\\doc\"",
